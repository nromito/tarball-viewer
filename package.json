{
  "name": "tarball-viewer",
  "displayName": "tarball-viewer",
  "description": "Allows you to inspect the contents of tarballs",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/nromito/tarball-viewer"
  },
  "publisher": "NickRomito",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tarball-viewer.readTarballFile",
        "title": "Read File"
      }
    ],
    "customEditors": [
      {
        "viewType": "tarball-viewer.tarballPreview",
        "displayName": "Tarball Viewer",
        "selector": [
          {
            "filenamePattern": "*.tgz"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "tar-stream": "^2.2.0"
  }
}
